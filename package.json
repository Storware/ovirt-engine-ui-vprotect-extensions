  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "prettify": "prettier --write \"**/*.ts*\"",
    "clean:dist": "rm -rf dist",
    "zip:oVirt": "cd dist && zip -r oVirt.zip ./*",
    "zip:openstack": "cd dist && zip -r openstack.zip ./*",
    "build:oVirt": "yarn clean:dist && webpack --config webpack.ovirt.prod.js",
    "build:openstack": "yarn clean:dist && webpack --config webpack.openstack.prod.js",
    "build-n-zip:oVirt": "yarn build:oVirt && yarn zip:oVirt",
    "build-n-zip:openstack": "yarn build:openstack && yarn zip:openstack"
  },
